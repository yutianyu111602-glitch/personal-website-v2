# 环境变量模板（复制为 .env.local 使用）

VITE_MUSIC_BASE_URL=http://localhost:9000
VITE_AUTOPILOT_ENABLED=true
VITE_AUTOPILOT_INTERVAL_MS=120000
VITE_BUS_SYNC_ENABLED=true
VITE_EMOTION_THEME_ENABLED=true
PORT=3000
TGR_PORT=3001
METADATA_PORT=3500
# 真实流媒体URL（主/备）；为空时 /api/stream_url 返回404
STREAM_PRIMARY=
STREAM_BACKUP=

# ===== 代理网关（由 metadata_server 反向代理，前端统一走 /api） =====
# Termusic 控制网关（HTTP）
TERMUSIC_GATEWAY=http://127.0.0.1:7533
# 音频分析网关（HTTP）
AUDIO_GATEWAY=http://127.0.0.1:18766

# ===== 开发联调 Mock 开关（上游未启动时可用） =====
# 在开发环境可设为 true，前端即可在未启动上游时照常联调
TERMUSIC_MOCK=false
AUDIO_MOCK=false

# ===== Secrets (迁移自旧版本，仅填写值，不要提交到仓库) =====
OPENAI_API_KEY=
OPENAI_ORG_ID=org-default
OPENAI_BASE_URL=https://api.openai.com/v1

# ===== Multi-Model Providers =====
DEEPSEEK_API_KEY=
DEEPSEEK_BASE_URL=https://api.deepseek.com

DASHSCOPE_API_KEY=
DASHSCOPE_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1

ANTHROPIC_API_KEY=
ANTHROPIC_BASE_URL=https://api.anthropic.com

# 自动召开专家会议
MEETING_AUTO_RUN=false


